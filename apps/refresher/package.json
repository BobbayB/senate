{
    "name": "@senate/refresher",
    "version": "1.0.0",
    "main": "./dist/index",
    "typings": "./dist/index",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "license": "MIT",
    "type": "module",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup",
        "start": "tsup && node dist/index.js",
        "dev": "tsup && node dist/index.js",
        "start:refresher": "tsup && node dist/index.js",
        "dev:refresher": "tsup && node dist/index.js",
        "clean": "rimraf .turbo node_modules dist",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "prettier": "prettier src --check",
        "prettier:fix": "prettier src --write"
    },
    "dependencies": {
        "@senate/common-types": "*",
        "@senate/config": "*",
        "@senate/database": "*",
        "@senate/tsconfig": "*",
        "node-cron": "^3.0.2",
        "node-fetch": "^3.3.0"
    },
    "devDependencies": {
        "@types/node-cron": "^3.0.6",
        "rimraf": "^3.0.2",
        "tsup": "^6.5.0",
        "tsx": "^3.12.1",
        "typescript": "^4.9.3"
    },
    "engines": {
        "node": "16"
    },
    "packageManager": "yarn@1.22.19"
}
