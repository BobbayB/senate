{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "module": "esnext",
    "jsx": "react-jsx",
    "noEmit": true,
    "incremental": true
  },
  "include": ["src", "next-env.d.ts"],
  "exclude": ["node_modules"]
}
